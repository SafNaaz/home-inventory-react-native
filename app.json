{
  "expo": {
    "name": "HiHome",
    "slug": "HiHome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8FAFC",
      "dark": {
        "image": "./assets/transparent.png",
        "backgroundColor": "#020617"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safnas.fridgeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.safnas.fridgeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e71839cd-4cd6-49e4-bd68-ffab2ccadcff"
      }
    },
    "plugins": [
      "expo-dev-client",
      "@react-native-community/datetimepicker"
    ],
    "newArchEnabled": true
  }
}
